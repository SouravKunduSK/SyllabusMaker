@model IEnumerable<SyllabusMaker.Models.CLO>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayoutPage.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Outcomes)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Course.CourseCode)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Outcomes)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Course.CourseCode)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CLOId }) |
            @Html.ActionLink("Details", "Details", new { id=item.CLOId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.CLOId })
        </td>
    </tr>
}

</table>
